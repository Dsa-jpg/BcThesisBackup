                  <![CDATA[import urllib
import urllib2
import mimetypes
import time
import json

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        self.file_path = '/home/nao/recordings/microphones/record.wav'
        self.api_url = 'https://openaiimg-250773176877.europe-west4.run.app/upload-non-streamed' #'http://160.217.213.158:5000/upload'  # Změň na URL tvého Flask server

        self.tts = ALProxy('ALTextToSpeech')
        self.ttsa = ALProxy('ALAnimatedSpeech')

    def onLoad(self):
        # Iniciální kód
        pass

    def onUnload(self):
        # Úklidový kód
        pass

    def onInput_onStart(self):
        self.send_file()  # Volání metody pro odeslání souboru
        pass

    def onInput_onStop(self):
        self.onUnload()
        self.onStopped()

    def send_file(self):
        # Odeslání souboru na server
        with open(self.file_path, 'rb') as file:
            file_data = file.read()
            boundary = '----WebKitFormBoundary7MA4YWxkTrZu0gW'
            body = (
                '--' + boundary + '\r\n'
                'Content-Disposition: form-data; name="file"; filename="record.wav"\r\n'
                'Content-Type: audio/wav\r\n\r\n' +
                file_data + '\r\n'
                '--' + boundary + '--\r\n'
            )

            headers = {
                'Content-Type': 'multipart/form-data; boundary=' + boundary,
                'Content-Length': str(len(body))
            }

            request = urllib2.Request(self.api_url, body, headers)

            try:
                response = urllib2.urlopen(request)
                response_data = json.loads(response.read())
                ai_completion = response_data['response']
                ai_completion = ai_completion.encode('utf-8')  # Encode to UTF-8 for printing
                movement = "random"
                #self.ttsa.pCall("say",ai_completion, {"speakingMovementMode":movement})
                #time.sleep(10)


                 # Spuštění asynchronní metody
                future = self.ttsa.pCall("say", ai_completion, {"speakingMovementMode": movement})

                # Smyčka čekající na dokončení metody
                while self.ttsa.isRunning(future):
                    time.sleep(0.1)

                #self.tts.say(ai_completion)
                self.logger.fatal(response.read())
                self.onStopped()

            except urllib2.HTTPError as e:
                self.logger.fatal(e.reason)
                print('HTTP Error:', e.code, e.reason)
                self.tts.say("Nerozuměl jsem zkus to znovu.")
                self.onStopped()
            except urllib2.URLError as e:
                self.logger.fatal(e.reason)
                print('URL Error:', e.reason)
                self.tts.say("Nerozuměl jsem zkus to znovu.")
                self.onStopped()]]>
